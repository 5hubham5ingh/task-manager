{
    "version": 2,
    "builds": [
        {
            "src": "./server.js",
            "use": "@versel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "./server.js"
        }
    ],
    "framework": "express"
}